{
    "compilerOptions": {
        "target": "ES2021",
        "lib": [
            "ES2021"
        ],
        "module": "ES2022",
        "moduleResolution": "Bundler",
        "types": [
            "@cloudflare/workers-types"
        ],
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}